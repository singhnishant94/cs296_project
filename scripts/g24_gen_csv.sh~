#!/bin/bash

#CSV File Generator
for i in $(seq 1 2000);
    do 
	for j in $(seq 1 4);
	    do
			{
			read v1 v2 v3 iter
			read v1 v2 v3 v4 v5 step_t v6
			read v1 v2 v3 v4 v5 coll_t v6
			read v1 v2 v3 v4 v5 v6 vel_t v7
			read v1 v2 v3 v4 v5 v6 pos_t v7
			read junk
			read v1 v2 v3 v4 loop_t v5
			} <"./data/g24out-$i-$j.txt"
			echo "$iter,$j,$step_t,$coll_t,$vel_t,$pos_t,$loop_t" >> ./data/g24_lab05data_01.csv
		   
	    done
    done
