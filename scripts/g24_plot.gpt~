set title "Average Step Times vs Iteration number"
set auto x
set yrange [0:15]
set xrange [-1:50]
set xlabel "No of iterations"
set ylabel "Time in ms"
set style data histogram
set style histogram cluster gap 1
set style fill solid border -1
set boxwidth 0.9
#set xtic rotate by -45 scale 0 font ",8"
#set scale 0
#set bmargin 10 
set datafile separator ","

set term png
set output "xyz.png"
 plot 'av_values.csv' using 2 title "Average Step Time" with histogram,\
      'av_values.csv' using 6 title "Average Loop Time" with lines
set term x11
