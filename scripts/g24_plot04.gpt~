#to plot the part 4 - frequency and cumulative - for iteration value 46 -- last two digits of 120050046
set terminal png
set output './plots/g24_plot04.png'
filename = './data/g24_lab05data_02.csv'
 
set datafile separator ","
 
bin(x, s) = s*int(x/s)
bin_width = 0.01
set boxwidth bin_width*0.6
set style fill solid 0.5
	 
set title "The Frequency and Cumulative Frequency of Step Time"
set xlabel "step time in ms"
set ylabel "frequency"
	 
plot filename every ::76*15+1::77*15 using (bin($3,bin_width)+bin_width/2):(1.0) smooth frequency title 'frequency' with boxes, \
	filename every ::76*15+1::77*15 using (bin($3,bin_width)+bin_width/2):(1.0) smooth cumulative title 'cumulative frequncy' with lines
