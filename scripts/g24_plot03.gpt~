#!/usr/bin/gnuplot

set title "Graph showing step times with errors"
set terminal png
set output "./plots/g24_plot03.png"
set datafile separator ','
set key right top

set xlabel "Iteration value"
set ylabel "Time(ms)
set yrange [] noreverse

set autoscale

plot "./data/stddev.csv" using 1:2:3 title "Errors"  with yerrorbars,\
     "./data/stddev.csv" using 1:2  title "Average Values" with smooth unique line
