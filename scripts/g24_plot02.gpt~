#!/usr/bin/gnuplot

#DATA="../data"

set title "Iterations vs various times"
set terminal png
set output "./plots/g24_plot02.png"
set datafile separator ','
set key right top

set xlabel "Iteration Value"
set ylabel "Time(ms)"

set yrange [] noreverse

set autoscale

plot "./data/g24_lab05data_01.csv" using 1:3 title "Average Step Time" with lines, "./data/g24_lab05data_01.csv" using 1:4 title " Average collision time" with lines, "./data/g24_lab05data_01.csv" using 1:5 title "Average Velocity Time" with lines, "./data/g24_lab05data_01.csv" using 1:6 title "Average position time"  with lines, "./data/g24_lab05data_01.csv" using 1:($4 + $5 +$6) title "Sum of Averages" with lines smooth unique

